        <item name="colorControlHighlight">@color/md_blue_500_75</item>​
        <item name="colorControlNormal">@color/md_blue_grey_500</item>
        <item name="colorSwitchThumbNormal">@color/md_green_A700</item>
        <item name="android:colorForeground">@color/md_black_1000_75</item>
        <item name="windowActionModeOverlay">true</item>
        <item name="actionModeBackground">@color/md_blue_500</item>

        <item name="android:windowBackground">@color/window_background</item>
    </style>

